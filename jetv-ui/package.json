{
  "name": "jetv-ui",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.15.1",
  "description": "A Vue 3 UI Library",
  "author": "MidnightCrowing",
  "license": "MIT",
  "main": "dist/jetv-ui.umd.js",
  "module": "dist/jetv-ui.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "dist/jet-icons.json"
  ],
  "scripts": {
    "gallery": "vite --config vite.config.dev.ts",
    "build": "vue-tsc -b && vite build --config vite.config.prod.ts",
    "icon:build": "node scripts/build-icons.js",
    "prepare:icons": "node scripts/copy-file.js",
    "postbuild": "pnpm prepare:icons"
  },
  "devDependencies": {
    "@antfu/eslint-config": "7.2.0",
    "@iconify/tools": "^5.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@rushstack/eslint-patch": "^1.15.0",
    "@types/node": "^25.2.0",
    "@unocss/preset-mini": "^66.6.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^14.2.0",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.7.0",
    "rollup-plugin-dts": "^6.3.0",
    "sass": "^1.97.3",
    "terser": "^5.46.0",
    "tslib": "^2.8.1",
    "typescript": "~5.9.3",
    "unocss": "^66.6.0",
    "unplugin-auto-import": "^21.0.0",
    "vite": "^7.3.1",
    "vue": "^3.5.27",
    "vue-router": "^5.0.2",
    "vue-tsc": "^3.2.4"
  }
}
