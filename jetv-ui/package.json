{
  "name": "jetv-ui",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.15.1",
  "description": "A Vue 3 UI Library",
  "author": "MidnightCrowing",
  "license": "MIT",
  "main": "dist/jetv-ui.umd.js",
  "module": "dist/jetv-ui.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "dist/jet-icons.json"
  ],
  "scripts": {
    "gallery": "vite --config vite.config.dev.ts",
    "build": "vue-tsc -b && vite build --config vite.config.prod.ts",
    "icon:build": "node scripts/build-icons.js",
    "prepare:icons": "node scripts/copy-file.js",
    "postbuild": "pnpm prepare:icons"
  },
  "devDependencies": {
    "@antfu/eslint-config": "5.2.1",
    "@iconify/tools": "^4.1.2",
    "@rollup/plugin-typescript": "^12.1.4",
    "@rushstack/eslint-patch": "^1.12.0",
    "@types/node": "^24.3.0",
    "@unocss/preset-mini": "^66.4.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^13.8.0",
    "eslint": "^9.34.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.4.0",
    "rollup-plugin-dts": "^6.2.3",
    "sass": "^1.91.0",
    "terser": "^5.43.1",
    "tslib": "^2.8.1",
    "typescript": "~5.9.2",
    "unocss": "^66.4.2",
    "unplugin-auto-import": "^20.1.0",
    "vite": "^7.1.3",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.6"
  }
}
